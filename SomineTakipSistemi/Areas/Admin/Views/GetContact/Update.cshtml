@model Entity.Concrete.GetContact
@{
    ViewData["Title"] = "Update";
    Layout = "~/Areas/Admin/Views/_adminLayout.cshtml";
}

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Quixlab - Bootstrap Admin Dashboard Template by Themefisher.com</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/adminimages/favicon.png">
    <!-- Custom Stylesheet -->
    <link href="~/admin/css/style.css" rel="stylesheet">

</head>

<body>
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Gelen Mesaj Güncelle</h4>
                <div class="basic-form">
                    <form method="post">
                        <div class="form-group">
                            <label>Ad Soyad</label>
                            <input name="Name" type="text" class="form-control input-default" placeholder="Örnek Başlık">
                            <br />
                            <label>Numara</label>
                            <input name="Number" type="text" class="form-control input-default" placeholder="Örnek Açıklama">
                            <br />
                            <label>Mail</label>
                            <input name="Mail" type="text" class="form-control input-default" placeholder="Örnek Açıklama">
                            <br />
                            <label>Konu</label>
                            <input name="Subject" type="text" class="form-control input-default" placeholder="Örnek Açıklama">
                            <br />
                            <label>Mesaj</label>
                            <input name="Message" type="text" class="form-control input-default" placeholder="Örnek Açıklama">
                            <br />
                            <br />
                            <button class="btn btn-primary">Kaydet</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</body>

</html>
